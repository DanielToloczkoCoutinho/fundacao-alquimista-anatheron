{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "regions": ["gru1"],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://fundacao-alquimista-anatheron.vercel.app"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_URL": "https://fundacao-alquimista-anatheron.vercel.app"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
