    if [ -f "$BASE_DIR/status_completo.sh" ]; then
        echo "âœ… status_completo.sh - SISTEMA VERIFICADO" >> "$SCRIPTS_FILE"
        ./status_completo.sh >> "$SCRIPTS_FILE" 2>/dev/null
        echo "" >> "$SCRIPTS_FILE"
    fi
    
    # Explorer - JÃ FUNCIONA  
    if [ -f "$BASE_DIR/explorer" ]; then
        echo "âœ… explorer - SISTEMA VERIFICADO" >> "$SCRIPTS_FILE"
        ./explorer >> "$SCRIPTS_FILE" 2>/dev/null
        echo "" >> "$SCRIPTS_FILE"
