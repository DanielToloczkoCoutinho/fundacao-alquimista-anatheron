# 2. ğŸ” ANÃLISE ZENNITH DIRETA - ARQUIVOS COM "ZENNITH"
analise_zennith_direta() {
    log "ğŸ” ANALISANDO ARQUIVOS ZENNITH..."
    
    ZENNITH_FILE="$ZENNITH_DIR/$TIMESTAMP/arquivos_zennith.txt"
    
    echo "ğŸ‘‘ ARQUIVOS ZENNITH - BASE DE CONHECIMENTO" > "$ZENNITH_FILE"
    echo "======================================" >> "$ZENNITH_FILE"
    
    # Todos os arquivos que mencionam Zennith
    echo "" >> "$ZENNITH_FILE"
    echo "ğŸ“š ARQUIVOS COM 'ZENNITH' (39 encontrados):" >> "$ZENNITH_FILE"
    find "$BASE_DIR" -type f -name "*zennith*" | while read file; do
        echo "ğŸ“„ $file" >> "$ZENNITH_FILE"
    done
    
    # ConteÃºdo dos principais scripts Zennith
    echo "" >> "$ZENNITH_FILE"
    echo "ğŸ¯ PRINCIPAIS SCRIPTS ZENNITH:" >> "$ZENNITH_FILE"
    
    if [ -f "$BASE_DIR/comando-zennith.sh" ]; then
        echo "" >> "$ZENNITH_FILE"
        echo "ğŸ”§ comando-zennith.sh:" >> "$ZENNITH_FILE"
        head -20 "$BASE_DIR/comando-zennith.sh" >> "$ZENNITH_FILE"
    fi
    
    if [ -f "$BASE_DIR/menu-zennith.sh" ]; then
        echo "" >> "$ZENNITH_FILE"
        echo "ğŸ“‹ menu-zennith.sh:" >> "$ZENNITH_FILE"
        head -15 "$BASE_DIR/menu-zennith.sh" >> "$ZENNITH_FILE"
    fi
    
    log "âœ… AnÃ¡lise Zennith direta: $ZENNITH_FILE"
}

