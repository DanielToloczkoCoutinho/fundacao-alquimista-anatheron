    if [ -f "$BASE_DIR/comando-zennith.sh" ]; then
        echo "âœ… comando-zennith.sh - SISTEMA VERIFICADO" >> "$SCRIPTS_FILE"
        ./comando-zennith.sh status >> "$SCRIPTS_FILE" 2>/dev/null
        echo "" >> "$SCRIPTS_FILE"
    fi
    
    log "âœ… Scripts Zennith ativados: $SCRIPTS_FILE"
